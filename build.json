{
    "destination": "web",
    "source": "src",
    "fileMetadata": {
        "default": {
            "section": "standard"
        },
        "rules": [{
            "metadata": {
                "section": "tracks"
            },
            "pattern": "tracks/**"
        }, {
            "metadata": {
                "section": "blog"
            },
            "pattern": "posts/**"
        }, {
            "metadata": {
                "section": "a-propos"
            },
            "pattern": "pages/a-propos/*"
        }]
    }
}
